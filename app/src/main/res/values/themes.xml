<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TestCaseApplicationForLabs" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#7DB9B3</item>
        <item name="colorPrimaryVariant">#7DB9B3</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TestCaseApplicationForLabs.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TestCaseApplicationForLabs.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TestCaseApplicationForLabs.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="CornerSize30Percent" parent="">
        <item name="cornerSize">30%</item>
    </style>
    <style name="CornerSize40Percent" parent="">
        <item name="cornerSize">40%</item>
    </style>
    <style name="CornerSize5Percent" parent="">
        <item name="cornerSize">5%</item>
    </style>
</resources>